{
   "compilerOptions": {
     "allowJs": true,
     "checkJs": false,
     "moduleResolution": "node",
     "lib": [
      "es5",
      "es6"
    ],
    "target": "es5",
     "outDir": "build",
     "esModuleInterop": true,
     "experimentalDecorators": true,
     "noImplicitAny": false,
     "emitDecoratorMetadata": true,
     "strictPropertyInitialization": false
   },
   "include": ["./typings", "src"],
   "exclude": ["node_modules"]
 }
 